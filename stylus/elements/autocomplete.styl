.autocomplete-wrapper
	position: relative
	margin-left: 46px
	margin-right: 30px
	.autocomplete
		bottom: 0px
		width: 100%
		background: white
		padding: 0
		border: 1px solid gainsboro-light
		&.Textcomplete-hidden
			display: none
		.Textcomplete-headline
			background: silver-dark
			@extend $small-font-size
			padding: 4px 8px
			text-transform: uppercase
		.Textcomplete-option
			padding: 4px 10px
		.Textcomplete-highlighted
			background: grape-dark
			color: white