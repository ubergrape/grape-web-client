.pmpopover.popover-left
	.inner-popover(style="box-shadow:0 1px 0px rgba(0,0,0,0.1); padding-bottom: 5px")
		h3
			=_('Manage pms')
	div.manager-navigation
		a.active-users(class=(tabSelection === 'active' ? 'selected' : ''))= _("Active")
		a.invited-users(class=(tabSelection === 'invited' ? 'selected' : ''))= _("Invited")
		a.deleted-users(class=(tabSelection === 'deleted' ? 'selected' : ''))= _("Deleted")
	.po-scroll-wrap
		ul
