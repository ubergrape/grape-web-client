ul.invite-list
	each invitee in items
		if (highlighted.indexOf(invitee) == -1)
			li.user(data-id='#{invitee.id}', class= selected == invitee ? 'selected' : '')
				a
					.item-inner-wrp
						.avatar-wrap
							img(src="#{invitee.avatar}", alt="#{invitee.username}", width='20', height='20')
							.avatar-status(class=(invitee.status === 16 ? 'online' : 'offline'))
						span.user-name
							| #{invitee.displayName}