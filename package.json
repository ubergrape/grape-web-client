  "scripts": {
    "all": "npm run test && npm run build",
    "start:dev": "webpack-dev-server --host 0.0.0.0 --port 8081 --hot --inline",
    "start:proxy": "node ./bin/proxy.js",
    "build": "webpack --bail",
    "build:lib": "babel src --out-dir lib",
    "build:watch": "webpack --progress --colors --watch",
    "build:i18n": "npm run clear:i18n && npm run build && npm run i18n:export",
    "build:analize": "cross-env ANALIZE=true NODE_ENV=production npm run build:watch",
    "test": "karma start",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "lint": "eslint ./src",
    "lint:staged": "lint-staged",
    "clear": "rimraf '{node_modules,npm-shrinkwrap.json}'",
    "clear:i18n": "rimraf 'i18n/{src,export}'",
    "i18n:export": "npm run build && npm run i18n:export:new && npm run i18n:export:legacy",
    "i18n:export:new": "babel-node i18n/scripts/export.js",
    "i18n:export:legacy": "babel-node i18n/scripts/legacyExport.js",
    "i18n:import": "npm run build && babel-node i18n/scripts/import.js",
    "prepublishOnly": "cross-env NODE_ENV=production npm run all",
    "sg": "cross-env STATIC_PATH=/dist/ styleguidist server",
    "sg:build": "styleguidist build"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "pre-commit": "lint:staged"
