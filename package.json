  "scripts": {
    "all": "npm run test && npm run build",
    "start:static": "npm run static & STATIC_PATH='http://127.0.0.1:8081/' npm run build:dist:watch",
    "start:dev": "webpack-dev-server",
    "build": "npm run build:dist && npm run build:lib",
    "build:watch": "npm run build:dist:watch && npm run build:lib:watch",
    "build:lib": "npm run clear:lib && npm run build:lib:jade && babel src --out-dir lib",
    "build:lib:jade": "cpy legacy/templates/**/*.jade ../lib --cwd=src --parents",
    "build:lib:watch": "npm run build:lib -- --watch src",
    "build:dist": "webpack --bail",
    "build:dist:watch": "webpack --progress --colors --watch",
    "build:i18n": "npm run clear:i18n && npm run build && npm run i18n:export",
    "test": "karma start",
    "test:watch": "NODE_ENV=test karma start",
    "lint": "eslint ./src",
    "lint:staged": "lint-staged",
    "static": "static ./dist --port 8081",
    "clear": "npm cache clear && rm -fr node_modules && rm -f npm-shrinkwrap.json",
    "clear:lib": "rm -fr ./lib/*",
    "clear:i18n": "rm -rf ./i18n/{src,export}",
    "shrinkwrap": "npm run clear && npm i --production --ignore-scripts && npm dedupe && npm i && npm shrinkwrap",
    "i18n:export": "npm run build && npm run i18n:export:new && npm run i18n:export:legacy",
    "i18n:export:new": "babel-node i18n/scripts/export.js",
    "i18n:export:legacy": "babel-node i18n/scripts/legacyExport.js",
    "i18n:import": "npm run build && babel-node i18n/scripts/import.js",
    "prepublish": "NODE_ENV=production npm run all",
    "sg": "STATIC_PATH=/dist/ styleguidist server",
    "sg:build": "styleguidist build"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "pre-commit": "lint:staged"
}
