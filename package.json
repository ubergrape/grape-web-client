{
  "name": "grape-web",
  "version": "0.40.0",
  "dependencies": {
    "jss": "^5.5.6",
    "jss-default-unit": "^4.1.4",
    "jss-expand": "^1.6.0",
    "jss-extend": "^2.0.6",
    "jss-isolate": "^1.0.0",
    "jss-props-sort": "^2.0.2",
    "jss-vendor-prefixer": "^3.0.3",
    "lodash": "^3.10.0",
    "react-jss": "4.2.0"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.5.8",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^5.1.4",
    "cpy": "^3.4.1",
    "es5-shim": "^4.1.10",
    "es6-shim": "^0.32.2",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-config-ubergrape": "^2.0.0",
    "eslint-plugin-react": "^3.9.0",
    "eslint-plugin-babel": "^2.1.1",
    "expect.js": "^0.3.1",
    "grape-js-emoji": "^0.0.1",
    "karma": "^0.13.8",
    "karma-chrome-launcher": "^0.1.10",
    "karma-cli": "^0.0.4",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^2.0.3",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "react-addons-test-utils": "^0.14.0",
    "svgo": "^0.6.0",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  },
  "scripts": {
    "all": "npm run lint && npm run test && npm run build",
    "clean": "rm -fr ./lib/*",
    "build": "npm run clean && npm run build:lib && npm run svg",
    "build:lib": "babel src --out-dir lib",
    "start": "NODE_ENV=development webpack-dev-server --hot",
    "lint": "eslint ./src",
    "svg": "npm run svg:clean && npm run svg:copy",
    "svg:clean": "svgo -f src/svg-icons/svg -o src/svg-icons/svg --enable removeTitle",
    "svg:copy": "cpy src/svg-icons/svg/*.svg lib/svg-icons/svg",
    "test": "karma start",
    "test:watch": "NODE_ENV=test karma start",
    "prepublish": "npm run all"
  }
}
