  "scripts": {
    "all": "yarn lint && yarn test && yarn build",
    "clean": "rm -fr ./lib/*",
    "build": "yarn clean && yarn svg && yarn build:lib",
    "build:lib": "babel src --out-dir lib",
    "build:lib:watch": "babel src --out-dir lib --watch",
    "lint": "eslint ./src --fix",
    "precommit": "lint-staged",
    "svg": "yarn svg:copy && yarn svg:export",
    "svg:copy": "cpy src/svg-icons/svg/*.svg lib/svg-icons/svg",
    "svg:export": "babel-node ./bin/generateIconsIndex.js",
    "test": "karma start",
    "test:watch": "NODE_ENV=test karma start",
    "prepublishOnly": "yarn all"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
