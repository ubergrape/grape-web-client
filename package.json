  "scripts": {
    "start:dev": "webpack-dev-server --host 0.0.0.0 --port 8081 --inline",
    "start:dev:full": "cross-env APP=full yarn start:dev",
    "start:dev:embedded": "cross-env APP=embedded yarn start:dev",
    "start:dev:all": "yarn start:dev",
    "start:proxy": "node ./bin/proxy.js",
    "build": "yarn clear:build && yarn build:lib && webpack --bail",
    "build:lib": "babel src --out-dir lib",
    "build:watch": "webpack --progress --colors --watch",
    "build:analize": "cross-env ANALIZE=true NODE_ENV=production yarn build:watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coveralls": "jest --coverage --collectCoverageFrom src/**/*.{js} && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint src",
    "lint:staged": "lint-staged",
    "clear:build": "rimraf '{dist,lib}'",
    "clear:i18n": "rimraf 'i18n/{src,export}'",
    "i18n:export": "yarn clear:i18n && yarn build && babel-node i18n/scripts/export.js",
    "i18n:import": "yarn clear:i18n && yarn build && babel-node i18n/scripts/import.js",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn test && cross-env NODE_ENV=production yarn build"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/jest-mocks/globalMocks.js",
      "jest-localstorage-mock"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/",
      "/data/"
    ],
    "testURL": "http://www.example.com",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest-mocks/fileMock.js"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
