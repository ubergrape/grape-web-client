  "scripts": {
    "all": "npm run lint && npm run test && npm run build",
    "start:static": "npm run static & npm run build:watch",
    "start:dev": "webpack-dev-server",
    "build": "npm run build:dist && npm run build:lib",
    "build:watch": "npm run build:dist:watch && npm run build:lib:watch",
    "build:lib": "npm run clear:lib && npm run build:lib:jade && babel src --out-dir lib",
    "build:lib:jade": "cpy legacy/templates/**/*.jade ../lib --cwd=src --parents",
    "build:lib:watch": "npm run build:lib -- --watch src",
    "build:dist": "webpack --bail",
    "build:dist:watch": "webpack --progress --colors --watch",
    "build:i18n": "npm run build:lib && npm run i18n-export && npm run build:dist && npm run i18n-legacy-export",
    "test": "karma start",
    "test:watch": "NODE_ENV=test karma start",
    "lint": "eslint ./src",
    "static": "static ./dist --port 8081",
    "dedupe": "npm dedupe 2> ./dedupe-whitelist.txt ",
    "clear": "npm cache clear && rm -fr node_modules && rm -f npm-shrinkwrap.json",
    "clear:lib": "rm -fr ./lib/* && npm run clear:i18n",
    "clear:i18n": "rm -rf ./i18n/{src, export}",
    "shrinkwrap": "npm run clear && npm i --production --ignore-scripts && npm run dedupe && npm i && npm shrinkwrap",
    "i18n-export": "./node_modules/.bin/babel-node i18n/scripts/export.js",
    "i18n-legacy-export": "./node_modules/.bin/babel-node i18n/scripts/legacyExport.js",
    "i18n-import": "npm run build && ./node_modules/.bin/babel-node i18n/scripts/import.js",
    "prepublish": "NODE_ENV=production npm run all"
  }
